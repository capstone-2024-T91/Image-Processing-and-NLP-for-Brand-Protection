{
  "name": "Phishing Email Reader",
  "version": "1.0.0",
  "description": "This is an extension to read your email andd determine wheather its fishing or not",
  "manifest_version": 3,
  "permissions": [
    "activeTab", 
    "storage",
    "https://3.14.250.99/"
  ],
  "icons": {
    "32": "icon32.png",
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js", "popup.js"]
    }
  ],

  "author": "Groupe 30",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Phishing Email Reader"
  }
}
